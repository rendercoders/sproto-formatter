{
  "name": "sproto-formatter",
  "displayName": "Format Sproto",
  "description": "causes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rendercoders/sproto-formatter.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{
      "id": "sproto",
      "extensions": [".sproto"],
      "aliases": ["Sproto Protocol"]
    }],
    "grammars": [{
      "language": "sproto",
      "scopeName": "source.sproto",
      "path": "./syntaxes/sproto.tmLanguage.json"
    }],
    "commands": [
      {
        "command": "format-sproto.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
